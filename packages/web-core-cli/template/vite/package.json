    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.52.1",
    "ts-jest": "^23.1.4",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.4",
