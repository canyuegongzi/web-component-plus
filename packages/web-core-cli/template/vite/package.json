  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "scripts": {
    "format": "prettier --write \"{src,apps,libs,test}/**/*.{tsx,ts}\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.{tsx,ts}\" --fix",
    "test": "jest -c packages/web-ui/jest.config.js",
    "start:debug": "nodemon --config nodemon-debug.json",
    "build:tsc": "cross-env NODE_ENV=production tsc -p tsconfig.build.json",
    "build": "cross-env NODE_ENV=production tsc -p tsconfig.build.json && vite build --base=./",
    "dev:vite:package": "yarn start",
    "build:rollup:package": "cross-env NODE_ENV=production rollup -c ./build/build.js",
    "dev:rollup:package": "cross-env NODE_ENV=production rollup -c ./build/dev.js",
    "start": "cross-env NODE_ENV=development vite",
    "serve": "vite preview"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 10",
    "npm": ">= 5"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@canyuegongzi/web-core-plus": "0.0.28",
    "reflect-metadata": "^0.1.13"
  },
